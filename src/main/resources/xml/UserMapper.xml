<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.greengramver1.user.UserMapper">
    <insert id="insUser">
        insert into user
        set uid = #{uid}
        , upw = #{upw}
        , nick_name = #{nickName}
        , pic = #{pic}
    </insert>
</mapper>